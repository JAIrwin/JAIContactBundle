{% extends 'JAIContactBundle::base.html.twig' %}

{% block contact %}
	<div class="page-header">
		<h1>Contact Me</h1>
	</div>
	{{ form_start(form) }}
	<div id='contact_form'>
		{{ form_row(form.fromName) }}
		<div style='display:none'>{{ form_row(form.fromEmail, {attr: {tabindex: '-1', autocomplete: 'off'} }) }}</div>
		{{ form_rest(form) }}
	</div>
	{{ form_end(form) }}
{% endblock %}